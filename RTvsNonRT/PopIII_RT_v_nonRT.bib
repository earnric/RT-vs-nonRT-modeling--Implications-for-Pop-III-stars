	number = {1},
	pages = {23},
	publisher = {IOP Publishing},
	title = {{Following The Cosmic Evolution Of Pristine Gas I: Implications For Milky Way Halo Stars}},
	url = {http://stacks.iop.org/0004-637X/834/i=1/a=23?key=crossref.c23980cb96e832d66c8269ffd7c3ab81 http://arxiv.org/abs/1611.00025 http://dx.doi.org/10.3847/1538-4357/834/1/23},
