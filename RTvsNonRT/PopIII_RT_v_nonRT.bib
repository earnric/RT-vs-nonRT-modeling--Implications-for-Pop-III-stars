	file = {:Users/earnric/Library/Application Support/Mendeley Desktop/Downloaded/Sarmento, Scannapieco, Pan - 2016 - Following the Cosmic Evolution of Pristine Gas. I. Implications for Milky Way Halo Stars.pdf:pdf},
